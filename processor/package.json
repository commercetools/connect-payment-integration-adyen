{
  "name": "payment-integration-adyen",
  "version": "5.17.0",
  "description": "Payment integration with Adyen",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "node_modules/.bin/nodemon -q dist/main.js",
    "lint": "prettier --check \"**/**/*.{ts,js,json}\" && eslint src",
    "lint:fix": "prettier --write \"**/**/*.{ts,js,json}\" && eslint --fix src",
    "build": "rm -rf /dist && tsc",
    "dev": "ts-node src/main.ts",
    "watch": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/main.ts\" --legacy-watch",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --detectOpenHandles --coverage",
    "connector:post-deploy": "node src/connectors/post-deploy.ts",
    "connector:pre-undeploy": "node src/connectors/pre-undeploy.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@adyen/api-library": "30.0.1",
    "@commercetools-backend/loggers": "24.13.0",
    "@commercetools/connect-payments-sdk": "0.27.1",
    "@fastify/autoload": "6.3.1",
    "@fastify/cors": "11.2.0",
    "@fastify/formbody": "8.0.2",
    "@fastify/http-proxy": "11.4.1",
    "@fastify/request-context": "6.2.1",
    "@fastify/static": "8.3.0",
    "@fastify/type-provider-typebox": "6.1.0",
    "@sinclair/typebox": "0.33",
    "dotenv": "17.2.3",
    "fastify": "5.6.2",
    "fastify-plugin": "5.1.0"
  },
  "devDependencies": {
    "@commercetools/composable-commerce-test-data": "13.12.0",
    "@jest/globals": "30.2.0",
    "@types/jest": "30.0.0",
    "@types/node": "25.0.3",
    "@typescript-eslint/eslint-plugin": "8.51.0",
    "@typescript-eslint/parser": "8.51.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jest": "29.12.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-unused-imports": "4.3.0",
    "jest": "30.2.0",
    "msw": "2.12.7",
    "node-fetch": "3.3.2",
    "nodemon": "3.1.11",
    "prettier": "3.7.4",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "overrides": {
    "path-to-regexp": "3.3.0"
  }
}
